#Cycled Data? (default = OFF)
*.Cycles: OFF

#Max Number of Events
MidUnpack.MaxEventNumber: 10

# Verbosity Levels 0 = none, 1 = errors, 2 = data dump, (default = 1)
MidUnpack.Verbosity: 1

# Kill the program on an error? Comes with a data dump of bad MIDAS event (default = YES)
MidUnpack.ErrorKill: YES

# Set the goodness of HPGe detectors (default = ON)
*.HPGe0: ON
*.HPGe1: OFF
*.HPGe2: ON
*.HPGe3: ON
*.HPGe10: OFF
*.HPGe15: ON
